{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "project": {
        "name": "ani-theme-ng5"
    },
    "apps": [
        {
            "root": "src",
            "outDir": "dist",
            "assets": [
                "assets",
                "favicon.ico",
                ".htaccess"
            ],
            "index": "index.html",
            "main": "main.ts",
            "polyfills": "polyfills.ts",
            "test": "test.ts",
            "tsconfig": "tsconfig.app.json",
            "testTsconfig": "tsconfig.spec.json",
            "prefix": "app",
            "styles": [
                "../node_modules/c3/c3.css",
                "../node_modules/angular2-toaster/toaster.css",
                "../node_modules/font-awesome/css/font-awesome.css",
                "../node_modules/fullcalendar/dist/fullcalendar.css",
                "../node_modules/perfect-scrollbar/css/perfect-scrollbar.css",
                "scripts/jb-editor/editor.css",
                "styles/themes/app-midnight-blue.scss"
            ],
            "scripts": [
                "../node_modules/jquery/dist/jquery.js",
                "scripts/jqvmap/jquery.vmap.js",
                "../node_modules/chart.js/dist/Chart.js",
                "scripts/jqvmap/maps/jquery.vmap.world.js",
                "../node_modules/moment/moment.js",
                "scripts/jb-editor/editor.js",
                "scripts/progressButton/classie.js",
                "scripts/progressButton/modernizr.custom.js",
                "scripts/progressButton/progressButton.js",
                "../node_modules/fullcalendar/dist/fullcalendar.js",
                "../node_modules/perfect-scrollbar/dist/perfect-scrollbar.js"
            ],
            "environmentSource": "environments/environment.ts",
            "environments": {
                "dev": "environments/environment.ts",
                "prod": "environments/environment.prod.ts"
            }
        }
    ],
    "e2e": {
        "protractor": {
            "config": "./protractor.conf.js"
        }
    },
    "lint": [
        {
            "project": "src/tsconfig.app.json"
        },
        {
            "project": "src/tsconfig.spec.json"
        },
        {
            "project": "e2e/tsconfig.e2e.json"
        }
    ],
    "test": {
        "karma": {
            "config": "./karma.conf.js"
        }
    },
    "defaults": {
        "styleExt": "scss",
        "component": {}
    }
}
